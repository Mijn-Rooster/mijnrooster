{
  "name": "mijnrooster-client",
  "version": "1.2.0",
  "description": "Mijn Rooster is een kiosk-applicatie voor het tonen van roosters op schermen",
  "main": "./out/main/index.js",
  "author": "David Jongeneel, Reinout Muis, Jonathan van der Pligt",
  "scripts": {
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "start": "electron-vite preview --noSandbox",
    "dev": "electron-vite dev --noSandbox",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@esm2cjs/is-online": "^10.0.0",
    "@types/w3c-web-hid": "^1.0.6",
    "auto-launch": "^5.0.6",
    "electron-log": "^5.3.1",
    "electron-updater": "^6.3.4"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.21",
    "electron": "^35.0.2",
    "electron-builder": "^25.1.8",
    "electron-devtools-installer": "^4.0.0",
    "electron-vite": "^3.0.0",
    "eslint": "^9.22.0",
    "eslint-plugin-svelte": "^3.1.0",
    "flowbite": "^3.1.2",
    "flowbite-svelte": "^0.48.4",
    "flowbite-svelte-icons": "^2.0.3",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "svelte": "^4.2.19",
    "svelte-google-materialdesign-icons": "^1.0.5",
    "tailwindcss": "^4.0.14",
    "vite": "^6.2.2"
  }
}
